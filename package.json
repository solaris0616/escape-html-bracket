{
    "name": "espace-html-bracket",
    "displayName": "espace-html-bracket",
    "description": "Espace HTML bracket",
    "version": "0.0.1",
    "publisher": "mahirun",
    "engines": {
        "vscode": "^1.22.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.escapeBracket"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.escapeBracket",
                "title": "Escape HTML bracket"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/clipboardy": "^1.1.0",
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6"
    },
    "dependencies": {
        "clipboardy": "^1.2.3"
    }
}
